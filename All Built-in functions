# ğŸš€ Python Built-in Functions Titan Code (Output in Comments)

# âœ… abs()
print(abs(-5))                     # â¡ï¸ 5

# âœ… all()
print(all([True, True, False]))    # â¡ï¸ False

# âœ… any()
print(any([False, False, True]))   # â¡ï¸ True

# âœ… ascii()
print(ascii('PavithranğŸ˜Š'))        # â¡ï¸ 'Pavithran\\U0001f60a'

# âœ… bin()
print(bin(10))                     # â¡ï¸ '0b1010'

# âœ… bool()
print(bool(0))                     # â¡ï¸ False

# âœ… bytearray()
b = bytearray("Hello", "utf-8")
b[0] = 74
print(b)                           # â¡ï¸ bytearray(b'Jello')

# âœ… bytes()
print(bytes("Python", "utf-8"))    # â¡ï¸ b'Python'

# âœ… callable()
print(callable(len))               # â¡ï¸ True

# âœ… chr()
print(chr(97))                     # â¡ï¸ 'a'

# âœ… complex()
print(complex(2, 3))               # â¡ï¸ (2+3j)

# âœ… divmod()
print(divmod(10, 3))               # â¡ï¸ (3, 1)

# âœ… enumerate()
print(list(enumerate(['a', 'b']))) # â¡ï¸ [(0, 'a'), (1, 'b')]

# âœ… eval()
print(eval('2 + 3 * 4'))           # â¡ï¸ 14

# âœ… filter()
print(list(filter(lambda x: x%2==0, [1,2,3,4])))  # â¡ï¸ [2, 4]

# âœ… float()
print(float('3.14'))               # â¡ï¸ 3.14

# âœ… format()
print(format(1234, ','))           # â¡ï¸ '1,234'

# âœ… frozenset()
print(frozenset([1,2,2,3]))        # â¡ï¸ frozenset({1, 2, 3})

# âœ… getattr()
class A:
    name = "Titan"
print(getattr(A, 'name'))          # â¡ï¸ 'Titan'

# âœ… globals()
print('globals keys:', list(globals().keys())[:5])  # â¡ï¸ shows first few global keys

# âœ… hasattr()
print(hasattr(A, 'name'))          # â¡ï¸ True

# âœ… hash()
print(hash('Python'))              # â¡ï¸ (Some integer hash value)

# âœ… hex()
print(hex(255))                    # â¡ï¸ '0xff'

# âœ… id()
x = 5
print(id(x))                       # â¡ï¸ (Memory address, e.g., 9789312)

# âœ… int()
print(int('10'))                   # â¡ï¸ 10

# âœ… isinstance()
print(isinstance(5, int))          # â¡ï¸ True

# âœ… issubclass()
class B(A): pass
print(issubclass(B, A))            # â¡ï¸ True

# âœ… iter() & next()
it = iter([1, 2, 3])
print(next(it))                    # â¡ï¸ 1
print(next(it))                    # â¡ï¸ 2

# âœ… len()
print(len("Python"))               # â¡ï¸ 6

# âœ… list()
print(list('abc'))                 # â¡ï¸ ['a', 'b', 'c']

# âœ… map()
print(list(map(lambda x: x*2, [1,2,3])))  # â¡ï¸ [2, 4, 6]

# âœ… max()
print(max([1, 4, 2]))              # â¡ï¸ 4

# âœ… min()
print(min([1, 4, 2]))              # â¡ï¸ 1

# âœ… ord()
print(ord('A'))                    # â¡ï¸ 65

# âœ… pow()
print(pow(2, 3))                   # â¡ï¸ 8
print(pow(2, 3, 5))                # â¡ï¸ 3 (8 % 5)

# âœ… print()
print("Hello Titan!")              # â¡ï¸ Hello Titan!

# âœ… reversed()
print(list(reversed([1, 2, 3])))   # â¡ï¸ [3, 2, 1]

# âœ… round()
print(round(3.567, 2))             # â¡ï¸ 3.57

# âœ… set()
print(set([1,2,2,3]))              # â¡ï¸ {1, 2, 3}

# âœ… sorted()
print(sorted([3,1,2]))             # â¡ï¸ [1, 2, 3]

# âœ… str()
print(str(123))                    # â¡ï¸ '123'

# âœ… sum()
print(sum([1,2,3]))                # â¡ï¸ 6

# âœ… tuple()
print(tuple([1,2,3]))              # â¡ï¸ (1, 2, 3)

# âœ… type()
print(type(5))                     # â¡ï¸ <class 'int'>

# âœ… zip()
print(list(zip([1,2,3], ['a','b','c'])))  # â¡ï¸ [(1, 'a'), (2, 'b'), (3, 'c')]
